    <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th>Movie</th>
            <th>Actor</th>
            <th>Actress</th>
            <th>Director</th>
            <th>Year</th>
            <th>Rating</th>
            <th>Trailer</th>
          </tr>
        </thead>
      
        <tbody class="table-dark">
          <% for(var i=0; i <data.length; i++) { %>
            <% if (data[i].movie == '') { %>
              <% continue %>
            <% } %> 
         <tr onclick="window.location='/<%=data[i].movie %>';" style="cursor: pointer;">
            <th scope="row"><%= i+1 %></th>
            <td><a href="/<%=data[i].movie %>"> <%= data[i].movie %>  </a> </td>
            <td><%= data[i].actor %></td>
            <td><%= data[i].actress %></td>
            <td><%= data[i].director %></td>
            <td><%= data[i].year %></td>
            <td> <i><%= data[i].rating %>/10</i></td>
            <td><a href="<%= data[i].trailor %>">Visit</a></td>
          </tr>
          <% } %>
        </tbody>
      </table>
      <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
